var ship_list = [
  //public ships,
  [101,102,201,202,301,302,303,304,401,402,403,404,405,406,501,502,503,504,505,506,507,508,509,510,601,602,603,604,605,606,607,608,609,610,611,612,613,701,702,703,704,705],
